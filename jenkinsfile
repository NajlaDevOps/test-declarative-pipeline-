pipeline {
    agent any

    stages {
        stage('SCM checkout') {
            steps {
            echo 'getting from github'
                echo 'checkout completed'
            }
        }
        stage('build') {
            steps {
                echo 'build completed'
            }
        } 
        stage('test') {
             steps {
                sh 'date'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy completed'
            }
        }
    }
}
